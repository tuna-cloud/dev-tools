<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.CheckBox?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.control.Separator?>
<?import javafx.scene.control.Spinner?>
<?import javafx.scene.control.SplitPane?>
<?import javafx.scene.control.Tab?>
<?import javafx.scene.control.TableColumn?>
<?import javafx.scene.control.TableView?>
<?import javafx.scene.control.TabPane?>
<?import javafx.scene.control.TextArea?>
<?import javafx.scene.control.TextField?>
<?import javafx.scene.control.ToggleButton?>
<?import javafx.scene.control.ToolBar?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.layout.ColumnConstraints?>
<?import javafx.scene.layout.GridPane?>
<?import javafx.scene.layout.RowConstraints?>
<?import javafx.scene.layout.VBox?>
<AnchorPane prefHeight="600.0" prefWidth="1000.0" xmlns="http://javafx.com/javafx/17"
            xmlns:fx="http://javafx.com/fxml/1" fx:controller="com.tuna.tools.fiddler.FiddlerController">
    <children>
        <SplitPane fx:id="splitPane" dividerPositions="0.75" AnchorPane.bottomAnchor="0.0" AnchorPane.leftAnchor="0.0"
                   AnchorPane.rightAnchor="0.0" AnchorPane.topAnchor="0.0">
            <items>
                <AnchorPane fx:id="leftPane" minWidth="300.0">
                    <children>
                        <VBox fx:id="mainViewVBox" prefHeight="598.0" prefWidth="745.0" AnchorPane.bottomAnchor="0.0"
                              AnchorPane.leftAnchor="0.0" AnchorPane.rightAnchor="0.0" AnchorPane.topAnchor="0.0">
                            <children>
                                <ToolBar maxHeight="-Infinity" minHeight="-Infinity" prefHeight="40.0"
                                         prefWidth="200.0">
                                    <items>
                                        <ToggleButton id="customToggleButton" onMouseClicked="#onWebServerStartClick"
                                                      GridPane.columnIndex="5" GridPane.rowIndex="1"/>

                                        <Separator orientation="VERTICAL" prefHeight="200.0"/>
                                        <Button mnemonicParsing="false" text="Save All"/>
                                        <Separator orientation="VERTICAL" prefHeight="200.0"/>
                                        <Button mnemonicParsing="false" text="Open"/>
                                        <Separator orientation="VERTICAL" prefHeight="200.0"/>
                                        <Button mnemonicParsing="false" onMouseClicked="#onClearAllRecords"
                                                text="Remove All"/>
                                        <Separator orientation="VERTICAL" prefHeight="200.0"/>
                                        <TextField fx:id="keyWord"/>
                                        <Button mnemonicParsing="false" text="Search" onMouseClicked="#onSearch"/>
                                        <Button mnemonicParsing="false" text="Request" onMouseClicked="#onHttpRequest"/>
                                    </items>
                                </ToolBar>
                                <TableView fx:id="mainViewTable" prefHeight="400.0" prefWidth="745.0">
                                    <columns>
                                        <TableColumn fx:id="colSeq" prefWidth="39.0" text="#"/>
                                        <TableColumn fx:id="colUrl" prefWidth="136.0" text="URL"/>
                                        <TableColumn fx:id="colHttpVersion" minWidth="8.0" prefWidth="93.0"
                                                     text="Http Version"/>
                                        <TableColumn fx:id="colMethod" prefWidth="71.0" text="Method"/>
                                        <TableColumn fx:id="colResult" prefWidth="63.0" text="Result"/>
                                        <TableColumn fx:id="colTime" prefWidth="48.0" text="Time"/>
                                        <TableColumn fx:id="colHost" prefWidth="75.0" text="Host"/>
                                        <TableColumn fx:id="colBodySize" prefWidth="75.0" text="Body Size"/>
                                        <TableColumn fx:id="colRemoteIp" prefWidth="75.0" text="Remote Ip"/>
                                    </columns>
                                </TableView>
                            </children>
                        </VBox>
                    </children>
                </AnchorPane>
                <AnchorPane fx:id="rightPane" minHeight="0.0" minWidth="200.0" prefHeight="160.0" prefWidth="100.0">
                    <children>
                        <TabPane layoutX="117.0" prefHeight="598.0" prefWidth="246.0" tabClosingPolicy="UNAVAILABLE"
                                 AnchorPane.bottomAnchor="0.0" AnchorPane.leftAnchor="0.0" AnchorPane.rightAnchor="0.0"
                                 AnchorPane.topAnchor="0.0">
                            <tabs>
                                <Tab text="Overview">
                                    <content>
                                        <AnchorPane minHeight="0.0" minWidth="0.0" prefHeight="180.0" prefWidth="288.0">
                                            <children>
                                                <TextArea fx:id="overView" editable="false" layoutX="23.0"
                                                          layoutY="117.0" prefHeight="566.5" prefWidth="246.0"
                                                          AnchorPane.bottomAnchor="0.0" AnchorPane.leftAnchor="0.0"
                                                          AnchorPane.rightAnchor="0.0" AnchorPane.topAnchor="0.0"/>
                                            </children>
                                        </AnchorPane>
                                    </content>
                                </Tab>
                                <Tab text="Settings">
                                    <content>
                                        <AnchorPane fx:id="settingsGridParent">
                                            <children>
                                                <GridPane fx:id="settingsGidPane" vgap="6.0"
                                                          AnchorPane.leftAnchor="10.0" AnchorPane.rightAnchor="10.0"
                                                          AnchorPane.topAnchor="20.0">
                                                    <columnConstraints>
                                                        <ColumnConstraints hgrow="SOMETIMES" minWidth="10.0"/>
                                                        <ColumnConstraints hgrow="SOMETIMES" minWidth="10.0"
                                                                           prefWidth="100.0"/>
                                                    </columnConstraints>
                                                    <rowConstraints>
                                                        <RowConstraints minHeight="10.0" prefHeight="30.0"
                                                                        vgrow="SOMETIMES"/>
                                                        <RowConstraints minHeight="10.0" vgrow="SOMETIMES"/>
                                                        <RowConstraints minHeight="10.0" prefHeight="30.0"
                                                                        vgrow="SOMETIMES"/>
                                                        <RowConstraints minHeight="10.0" prefHeight="30.0"
                                                                        vgrow="SOMETIMES"/>
                                                        <RowConstraints minHeight="10.0" prefHeight="30.0"
                                                                        vgrow="SOMETIMES"/>
                                                        <RowConstraints minHeight="10.0" prefHeight="30.0"
                                                                        vgrow="SOMETIMES"/>
                                                        <RowConstraints minHeight="10.0" prefHeight="30.0"
                                                                        vgrow="SOMETIMES"/>
                                                        <RowConstraints minHeight="10.0" prefHeight="30.0"
                                                                        vgrow="SOMETIMES"/>
                                                    </rowConstraints>
                                                    <children>
                                                        <Label text="Port:"/>
                                                        <Spinner fx:id="bindPort" editable="true"
                                                                 GridPane.columnIndex="1"/>
                                                        <Label text="Https:" GridPane.rowIndex="1"/>
                                                        <CheckBox fx:id="https" mnemonicParsing="false" selected="true"
                                                                  GridPane.columnIndex="1" GridPane.rowIndex="1"/>
                                                        <Label text="Act as system proxy on startup:"
                                                               GridPane.rowIndex="2"/>
                                                        <CheckBox fx:id="systemProxySet" mnemonicParsing="false"
                                                                  selected="true" GridPane.columnIndex="1"
                                                                  GridPane.rowIndex="2"/>
                                                        <Label text="Allow remote connection:" GridPane.rowIndex="3"/>
                                                        <CheckBox fx:id="remoteEnable" mnemonicParsing="false"
                                                                  selected="true" GridPane.columnIndex="1"
                                                                  GridPane.rowIndex="3"/>
                                                        <Label text="HTTP2 support:" GridPane.rowIndex="4"/>
                                                        <CheckBox fx:id="http2Enable" mnemonicParsing="false"
                                                                  selected="true" GridPane.columnIndex="1"
                                                                  GridPane.rowIndex="4"/>
                                                        <Label text="Host KeyWord:" GridPane.rowIndex="5"/>
                                                        <TextArea fx:id="byPass" prefHeight="200.0" prefWidth="200.0"
                                                                  GridPane.columnIndex="1" GridPane.rowIndex="5"/>
                                                        <Label text="Regen Root Certificate:" GridPane.rowIndex="6"/>
                                                        <Label text="Export Root Certificate:" GridPane.rowIndex="7"/>
                                                        <Button mnemonicParsing="false" text="Reset"
                                                                GridPane.columnIndex="1" GridPane.rowIndex="6"/>
                                                        <Button mnemonicParsing="false" text="Export"
                                                                GridPane.columnIndex="1" GridPane.rowIndex="7"/>
                                                    </children>
                                                </GridPane>
                                            </children>
                                        </AnchorPane>
                                    </content>
                                </Tab>
                            </tabs>
                        </TabPane>
                    </children>
                </AnchorPane>
            </items>
        </SplitPane>
    </children>
</AnchorPane>
